
@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<body>

   

    <div id="map" style="position:absolute;right:10px; height:650px; width:700px;"></div>

    <script>
        function myMap() {
            var mapOptions = {
                center: new google.maps.LatLng(28.6139391, 77.20902120000005),
                zoom: 10,
                mapTypeId: google.maps.MapTypeId.HYBRID
            }
            var map = new google.maps.Map(document.getElementById("map"), mapOptions);
        }
    </script>
    
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAnrll05c6KvXmuSMRHuZDxn5-U6Y16sUI&callback=myMap"></script>
